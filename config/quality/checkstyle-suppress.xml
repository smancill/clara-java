<!DOCTYPE suppressions PUBLIC
    "-//Puppy Crawl//DTD Suppressions 1.1//EN"
    "http://www.puppycrawl.com/dtds/suppressions_1_1.dtd">

<suppressions>
  <!-- ignore generated Protobuf sources -->
  <suppress checks=".*" files="generated[\\/]source[\\/]proto"/>

  <!-- ignore Javadoc -->
  <suppress checks="Javadoc.*" files="org[\\/]jlab[\\/]clara[\\/]base[\\/]core"/>
  <suppress checks="Javadoc.*" files="org[\\/]jlab[\\/]clara[\\/]msg[\\/]sys[\\/].+[\\/]"/>
  <suppress checks="Javadoc.*" files="org[\\/]jlab[\\/]clara[\\/]sys[\\/]ccc"/>
  <suppress checks="Javadoc.*" files="org[\\/]jlab[\\/]clara[\\/]sys[\\/]report"/>
  <suppress checks="Javadoc.*" files="org[\\/]jlab[\\/]clara[\\/]util"/>
  <suppress checks="Javadoc.*" files=".*Test\.java"/>

  <!-- TODO remove once checkstyle#7064 is done -->
  <suppress checks="LineLength" files="org[\\/]jlab[\\/]clara[\\/]util[\\/]logging"/>
</suppressions>
